# Variables de entorno para el sistema de citas médicas

# URL de Supabase
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# URLs de los webhooks de n8n
N8N_WEBHOOK_DISPONIBILIDAD_URL=https://tu-instancia-n8n.com/webhook/disponibilidad
N8N_CREAR_CITA_URL=https://tu-instancia-n8n.com/webhook/crear-cita
N8N_MODIFICAR_CITA_URL=https://tu-instancia-n8n.com/webhook/modificar-cita
N8N_ELIMINAR_CITA_URL=https://tu-instancia-n8n.com/webhook/eliminar-cita

# Configuración de Google Calendar (para n8n)
GOOGLE_CALENDAR_CREDENTIALS={"type": "service_account", ...}
GOOGLE_CALENDAR_ID=primary

# Configuración de Telegram (opcional, para notificaciones)
TELEGRAM_BOT_TOKEN=your-telegram-bot-token
TELEGRAM_CHAT_ID=your-chat-id

# Configuración del servidor
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret

# Configuración de la base de datos (para migraciones)
DATABASE_URL=postgresql://user:password@localhost:5432/yourdb
